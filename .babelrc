{
  "presets": [["env", {
    "modules": false,
    "targets": {
      "browsers": [
        "last 1 major version",
        ">= 1%",
        "Chrome >= 45",
        "Firefox >= 38",
        "Edge >= 12",
        "Explorer >= 10",
        "iOS >= 9",
        "Safari >= 9",
        "Android >= 4.4",
        "Opera >= 30"
      ]
    }
  }]],
  "env": {
    "test": {
      "presets": [["env"]],
      "plugins": [
        "transform-runtime"
      ]
    }
  },
  "plugins": [
    "external-helpers",
    ["transform-es2015-classes", {
      "loose": true
    }]
  ]
}
